{
  "name": "shoppinglist-electron-app",
  "productName": "ShoppingList Electron app",
  "version": "1.0.0",
  "description": "A Typical Shopping List application build with ElectronJS",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-all": "electron-packager . --all",
    "package-win": "electron-packager . shoppinglist-electron-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Shopping List\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sdivakarrajesh/ShoppingList-Electron-App.git"
  },
  "author": "Divakar Rajesh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sdivakarrajesh/ShoppingList-Electron-App/issues"
  },
  "homepage": "https://github.com/sdivakarrajesh/ShoppingList-Electron-App#readme",
  "dependencies": {
    "electron": "^18.3.7"
  },
  "devDependencies": {
    "electron-packager": "^12.1.2"
  }
}
