{
  "name": "shoppinglist-electron-app",
  "version": "1.0.0",
  "description": "A Typical Shopping List application build with ElectronJS",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . --overwrite --asar --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Shopping List\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sdivakarrajesh/ShoppingList-Electron-App.git"
  },
  "author": "Divakar Rajesh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sdivakarrajesh/ShoppingList-Electron-App/issues"
  },
  "homepage": "https://github.com/sdivakarrajesh/ShoppingList-Electron-App#readme",
  "dependencies": {
    "electron": "^3.0.2"
  },
  "devDependencies": {
    "electron-packager": "^12.1.2"
  }
}
